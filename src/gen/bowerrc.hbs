{
  "registry": {
    {{#if registry_publish}}
    "publish": "{{{registry_publish}}}",
    {{/if}}
    {{#if registry_register}}
    "register": "{{{registry_register}}}",
    {{/if}}
    {{#if registry_search}}
    "search": [
      {{#registry_search}}
      "{{{search}}}"{{#unless @last}},{{/unless}}
      {{/registry_search}}
    ]
    {{/if}}
  },
  "storage": {
    {{#if storage_cache}}
    "cache": "{{{storage_cache}}}",
    {{/if}}
    {{#if storage_registry}}
    "registry": "{{{storage_registry}}}"
    {{/if}}
  }
  
  {{#if analytics}}
  ,"analytics": {{{analytics}}}
  {{/if}}
  
  {{#if ca}}
  ,"ca": "{{{ca}}}"
  {{/if}}
  
  {{#if color}}
  ,"color": "{{{color}}}"
  {{/if}}
  
  {{#if cwd}}
  ,"cwd": "{{{cwd}}}"
  {{/if}}
  
  {{#if directory}}
  ,"directory": "{{{directory}}}"
  {{/if}}
  
  {{#if https-proxy}}
  ,"https-proxy": "{{{https-proxy}}}"
  {{/if}}
  
  {{#if interactive}}
  ,"interactive": {{{interactive}}}
  {{/if}}
  
  {{#if proxy}}
  ,"proxy": "{{{proxy}}}"
  {{/if}}
  
  {{#if shorthand-resolver}}
  ,"shorthand-resolver": "{{{shorthand-resolver}}}"
  {{/if}}
  
  {{#if strict-ssl}}
  ,"strict-ssl": {{{strict-ssl}}}
  {{/if}}
  
  {{#if timeout}}
  ,"timeout": {{{timeout}}}
  {{/if}}
  
  {{#if tmp}}
  ,"tmp": "{{{tmp}}}"
  {{/if}}
  
  {{#if user-agent}}
  ,"user-agent": "{{{user-agent}}}"
  {{/if}}
}